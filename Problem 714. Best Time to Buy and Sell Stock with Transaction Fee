class Solution:
    def maxProfit(self, prices: List[int], fee: int) -> int:
        res =0
        minprice = prices[0]
        maxprice = 0
        for i in range(1,len(prices)):
            if prices[i] < minprice:
                minprice = prices[i]
            if prices[i] > minprice + fee:
                res += prices[i] - minprice - fee;
                minprice = prices[i] - fee;
                
        return res
